<!DOCTYPE html>
<html dir="ltr"
      xmlns:content="http://purl.org/rss/1.0/modules/content/"
      xmlns:dc="http://purl.org/dc/terms/"
      xmlns:foaf="http://xmlns.com/foaf/0.1/"
      xmlns:og="http://ogp.me/ns#"
      xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
      xmlns:sioc="http://rdfs.org/sioc/ns#"
      xmlns:sioct="http://rdfs.org/sioc/types#"
      xmlns:skos="http://www.w3.org/2004/02/skos/core#"
      xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon" href="~/Content/images/favicon.ico" type="image/vnd.microsoft.icon" />
    <title>BART Perks - @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <meta name="MobileOptimized" content="width">
    <meta name="HandheldFriendly" content="true">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
</head>
<body class="html not-front not-logged-in no-sidebars perk2-not-logged-in perk2-pages cus-user-anonymous cus-hide-breadcrumb @ViewBag.BodyClasses">
    <a href="#main-content" class="element-invisible element-focusable utility-nav">Skip to main content</a>
    <div id="errorMessage" style="display:none;">@ViewBag.ErrorMessage</div>
    <div id="errorDetails" style="display:none;">@ViewBag.ErrorDetails</div>
    <div id="modal">
        <div class="overlay"></div>
        <div class="modal-message-container">
            <div id="modal-message">
                <!-- message here -->
            </div>
        </div>
    </div>
    <div class="page-wrap">
        <div class="page">
            <header id="header" class="header">
                <div class="hd-inner xl-dpr xl-dpl pge-wdt clearfix">
                    <div class="logo-wrp fl">
                        <div class="logo-img">
                            <a href="/" title="Home" rel="home" id="logo">
                                <img src="~/Content/images/logo.png" alt="Home" />
                            </a>
                        </div>
                    </div>
                    <div class="mb-menu">
                        <div class="mb-icon">
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </div>
                    <div class="search-bar lg-fr">
                        <div class="search-icon"></div>
                        <div class="user-icon-block">
                        </div>
                    </div>
                    <div class="fl lg-fr nav-hd">
                        <div class="region region-header">
                            <div id="block-bart-acquia-search-bart-acquia-search-block" class="block block-bart-acquia-search srch-blk">
                                <div class="content">
                                    <form class="search-form" action="/siteinfo/search" method="post" id="search-form" accept-charset="UTF-8">
                                        <div>
                                            <div class="container-inline form-wrapper" id="edit-basic">
                                                <div class="form-item form-type-textfield form-item-keys">
                                                    <label for="edit-keys">Enter terms </label>
                                                    <input type="text" id="edit-keys" name="keys" value="" size="20" maxlength="255" class="form-text" />
                                                </div>
                                                <input type="submit" id="edit-submit" name="op" value="Search" class="form-submit" /><input type="hidden" name="get" value="[]" />
                                            </div><input type="hidden" name="form_build_id" value="form-RgmcWRe9y8uJdQUNZoaVcKR40osfWTA3HdZS0einZq0" />
                                            <input type="hidden" name="form_id" value="apachesolr_search_custom_page_search_form" />
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div id="block-system-main-menu" class="block block-system main-menu block-menu">
                                <div class="content">
                                    <ul class="menu">
                                        <li class="first expanded menu-expand-link">
                                            <a href="/schedules/bystation" title="Schedules" class="bart-menu-header">Schedules</a>
                                        </li>
                                        <li class="expanded menu-expand-link">
                                            <a href="/stations" title="Stations" class="bart-menu-header">Stations</a>
                                        </li>
                                        <li class="expanded menu-expand-link">
                                            <a href="/guide" title="Rider Guide" class="bart-menu-header">USING BART</a>
                                        </li>
                                        <li class="expanded menu-expand-link">
                                            <a href="/tickets" title="Tickets">FARES</a>
                                        </li>
                                        <li class="expanded menu-expand-link">
                                            <a href="/news/articles" title="News" class="bart-menu-header">News</a>
                                        </li>
                                        <li class="last expanded active-trail menu-expand-link">
                                            <a href="/about" title="About BART" class="bart-menu-header bart-mega-menu active-trail active">About</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <div class="breadcrumb-iner pge-wdt clearfix">
                <h2 class="element-invisible">You are here</h2>
                <div class="breadcrumb"><a href="/">Home</a></div>
            </div>
            <div id="main" class="pge-wdt clearfix">
                <div class="main-content-wrap">
                    <div class="main-content">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <footer id="footer" class="footer">
                <div class="footer-inner pge-wdt clearfix">
                    <div class="region region-footer">
                        <div id="block-menu-block-3" class="block block-menu-block quick-lnks md-fl lg-fr">
                            <h2>Quick Links</h2>
                            <div class="content">
                                <div class="menu-block-wrapper menu-block-3 menu-name-menu-quick-links parent-mlid-0 menu-level-1">
                                    <ul class="menu">
                                        <li class="first leaf menu-mlid-8296"><a href="/">Trip Planner</a></li>
                                        <li class="leaf menu-mlid-8281"><a href="/schedules/eta">Real Time Departures</a></li>
                                        <li class="leaf menu-mlid-8286"><a href="/schedules/bystation">Schedules by Station</a></li>
                                        <li class="leaf menu-mlid-8271"><a href="/tickets/calculator">Fare Calculator</a></li>
                                        <li class="leaf menu-mlid-8291"><a href="/schedules/advisories">Service Notifications</a></li>
                                        <li class="leaf menu-mlid-8276"><a href="/guide/parking">Parking</a></li>
                                        <li class="leaf menu-mlid-8256"><a href="/guide/airport">Airport Service</a></li>
                                        <li class="leaf menu-mlid-8251"><a href="/guide/accessibility">Accessible Services</a></li>
                                        <li class="leaf menu-mlid-8266"><a href="/about/jobs">Careers</a></li>
                                        <li class="leaf menu-mlid-8261"><a href="/about/bod/multimedia">Board Meetings</a></li>
                                        <li class="leaf menu-mlid-8241"><a href="/about/police">BART Police</a></li>
                                        <li class="last leaf menu-mlid-8301"><a href="http://bartable.bart.gov/">Visit BARTable</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div id="block-block-1" class="block block-block cnt-fter md-fl">
                            <div class="content">
                                <div class="contact">
                                    <h1>Bay Area Rapid Transit District (BART)</h1><a href="/siteinfo/contact">Contact Us</a>
                                    <h2>Stay Connected</h2>
                                    <ul>
                                        <li><a href="http://www.twitter.com/sfbart" rel="external" target="_blank"><img alt="twitter" src="~/Content/images/icon-white-twitter.svg" title="twitter" /></a></li>
                                        <li><a href="http://facebook.com/bartsf" rel="external" target="_blank"><img alt="facebook" src="~/Content/images/icon-white-facebook.svg" title="facebook" /></a></li>
                                        <li><a href="http://www.youtube.com/bartable" rel="external" target="_blank"><img alt="youtube" src="~/Content/images/icon-white-youtube.svg" title="youtube" /></a></li>
                                        <li><a href="http://www.pinterest.com/sfbart" rel="external" target="_blank"><img alt="pinterest" src="~/Content/images/icon-white-pinterest.svg" title="pinterest" /></a></li>
                                        <li><a href="/news/alerts/"><img alt="alerts" src="~/Content/images/icon-white-email.svg" title="alerts" /></a></li>
                                        <li><a href="/news/rss/"><img alt="rss" src="~/Content/images/icon-white-rss.svg" title="rss" /></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
            <footer id="footer_bottom" class="footer-bottom">
                <div class="footer-bottom-inner pge-wdt clearfix">
                    <div class="region region-footer-bottom">
                        <div id="block-bart-bart-footer" class="block block-bart">
                            <div class="content">
                                <div class="cpy-ftr-lnks clearfix am">
                                    <div class="rft-cpy lg-fr">
                                        <div id="locales" class="local-lang">
                                            <ul>
                                                <li><a href="/guide/spanish">Español</a></li>
                                                <li><a href="/guide/korean">한국어</a></li>
                                                <li><a href="/guide/italian">Italiano</a></li>
                                                <li><a href="/guide/french">Français</a></li>
                                                <li><a href="/guide/german">Deutsch</a></li>
                                                <li><a href="/guide/chinese">中文</a></li>
                                                <li><a href="/guide/japanese">日本語</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="lft-cpy lg-fl">
                                        <div id="footer-utility-nav" class="footer-utility-nav cpy-rgts lg-dpl"><a href="/siteinfo/privacy">Privacy Policy</a></div>
                                        <div id="copyright" class="cpy-rgts copyright"><a href="/siteinfo/copyright">&copy; @DateTime.Now.Year San Francisco Bay Area Rapid Transit District</a></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    @Scripts.Render("~/bundles/javascript");
</body>
</html>
